{
    "name": "Element Clustering",
    "version": "1.0",
    "description": "Highlight the elements that can be removed to reduce layout duration.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contents.js"
            ],
            "run_at": "document_end"
        }
    ],
    "manifest_version": 2
}